build:
	./rcc.sh build

build-no-cache:
	./rcc.sh build-no-cache

build-use-cache:
	./rcc.sh build-use-cache

evaluate:
	./rcc.sh evaluate 
	
init:
	./rcc.sh init

remove-docker-image:
	./rcc.sh remove-docker-image

reset-data-folder:
    ./rcc.sh reset-data-folder

reset-program-folder:
    ./rcc.sh reset-program-folder

reset-config-file:
    ./rcc.sh reset-config-file

reset-dockerfile:
    ./rcc.sh reset-dockerfile

reset-rcc-script:
    ./rcc.sh reset-rcc-script

run:
	./rcc.sh run

run-interactive:
	./rcc.sh run-interactive

run_and_stop:
	./rcc.sh run_and_stop

stop:
	./rcc.sh stop
